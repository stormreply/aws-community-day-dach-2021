{
  "command": {
    "publish": {
      "conventionalCommits": true
    },
    "version": {
      "allowBranch": "main",
      "conventionalCommits": true,
      "exact": true,
      "message": "chore(aws-community-day-dach-2021): publish %v [skip ci]",
      "noCommitHooks": true,
      "tagVersionPrefix": ""
    }
  },
  "granularPathspec": false,
  "npmClient": "yarn",
  "packages": [
    "constructs/*",
    "packages/*"
  ],
  "useWorkspaces": true,
  "version": "0.3.0"
}
